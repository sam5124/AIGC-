{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@astro/build",
        "config": {
          "command": "npm run build",
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }